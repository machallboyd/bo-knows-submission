{
  "manifest_version": 2,
  "name": "Bo Knows Submission",
  "permissions": [
    "https://www.bo-ne.ws/submit",
    "activeTab"
  ],
  "version": "1.0",
  "description" : "Chrome automation for submitting to Bo News",
  "default_locale": "en",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["jquery-3.2.1.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "icon128.png" }
}